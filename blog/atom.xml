---
layout: feed
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>BadgeChain Project Blog</title>
	<link href="http://utls.github.io/badgechain/blog/atom.xml" rel="self"/>
	<link href="http://utls.github.io/badgechain/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://utls.github.io/badgechain/blog</id>
	<author>
		<name>BadgeChain Project</name>
		<email>maxwell.lincoln@austin.utexas.edu</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://utls.github.io/badgechain/{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://utls.github.io/badgechain/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>